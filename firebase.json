{
  "hosting": [
    {
      "target": "api",
      "public": "functions/public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "moleculerApi"
        }
      ]
    }
  ]
}